= stylesheet_link_tag "categories"
.wrapper
  .container
    = render "/layouts/menu"
    .header
      .header-title 
        = "#{@category}"
    = render "/layouts/share"
    .categories-content
      - @pages.each do |page|
        .content-box
          .categories-text-box
          =link_to "#{page.title}", {:controller => "otherPages", :action => "show", :id => page.id}
  = render "/layouts/footer"

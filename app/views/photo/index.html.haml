= stylesheet_link_tag "photos"
.wrapper
  = render "/layouts/menu"

  .container
    .header
      .header-title "#{@album_name} Photos" 
    .content
      - @photos.each do |photo|
        .content-box
          .img-box
            %img{:src => book.picture.url}
          .text-box
      
  = render "/layouts/footer" 


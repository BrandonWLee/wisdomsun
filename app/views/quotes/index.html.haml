= stylesheet_link_tag "quotes", :media => "all"
.wrapper
  .container
    = render "/layouts/menu"
    .header
      .header-title= "Notes & Quotes"
    = render "/layouts/share"
    .content
      .quotes-box
        - @quotes.each do |quote|
          .content-box
            .quote-box
              .quote= "#{quote.quote}"
              .source= "#{quote.source}"


